{
  "name": "site_ejt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "ts-node prisma/seed.ts",
    "test:security": "ADMIN_USER=test_admin ADMIN_PASSWORD=test_password NODE_ENV=development jest tests/security --config jest.config.ts",
    "test:e2e": "cypress run",
    "test:load": "artillery run tests/load/load-test.yml",
    "test:load:debug": "artillery run tests/load/load-test-debug.yml",
    "test:all": "npm run test:security && npm run test:e2e && npm run test:load",
    "db:clean": "ts-node scripts/clean-test-members.ts",
    "db:update-roles": "ts-node scripts/update-roles.ts",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@fontsource/poppins": "^5.0.8",
    "@fontsource/roboto": "^5.0.8",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "^5.2.0",
    "@types/nodemailer": "^6.4.17",
    "framer-motion": "^10.16.1",
    "lucide-react": "^0.537.0",
    "next": "13.4.19",
    "nodemailer": "^7.0.5",
    "react": "18.2.0",
    "react-countup": "^6.4.2",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.16.0",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/cypress": "^0.1.6",
    "@types/jest": "^30.0.0",
    "@types/node": "20.5.3",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/react-slick": "^0.23.10",
    "artillery": "^2.0.24",
    "autoprefixer": "10.4.15",
    "cross-fetch": "^4.1.0",
    "cypress": "^14.5.4",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "jest": "^30.0.5",
    "postcss": "8.4.28",
    "prisma": "^5.2.0",
    "tailwindcss": "3.3.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "5.1.6"
  }
}
